<!-- The header ejs template is included to ensure our CSS and fonts are loaded -->
<%- include('partials/header') %>


    <div id="leaderboard_main_box">
        <h1>Leaderboard</h1>
        <table>
            <thead>
                <tr>
                    <th>Rank</th>
                    <th>Strikes</th>
                    <th>Date and Time</th>
                </tr>
            </thead>
            <tbody>
                <% leaderboard.forEach((entry, index) => { %>
                    <tr>
                        <td><%= index + 1 %></td>
                        <td><%= entry.streak %></td>
                        <td><%= entry.timestamp %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
        <a href="/">Back to Home</a>
    </div>


</body>
</html>
